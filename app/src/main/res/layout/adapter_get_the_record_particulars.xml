<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:orientation="vertical"    android:layout_width="match_parent"    android:layout_height="wrap_content">    <RelativeLayout        android:layout_width="wrap_content"        android:layout_height="wrap_content">        <com.makeramen.roundedimageview.RoundedImageView            android:id="@+id/adapter_get_the_record_particulars_list_image"            android:layout_marginTop="14dp"            android:layout_marginLeft="15.5dp"            app:riv_oval="true"            android:scaleType="centerCrop"            android:layout_width="50dp"            android:layout_height="50dp"/>        <TextView            android:id="@+id/adapter_get_the_record_particulars_list_name"            android:layout_toRightOf="@id/adapter_get_the_record_particulars_list_image"            android:layout_marginTop="32dp"            android:layout_marginLeft="16dp"            android:layout_width="wrap_content"            android:layout_height="wrap_content"/>        <TextView            android:id="@+id/adapter_get_the_record_particulars_list_time"            android:layout_marginTop="34.5dp"            android:layout_alignParentRight="true"            android:layout_marginRight="15dp"            android:layout_width="wrap_content"            android:layout_height="wrap_content"/>    </RelativeLayout>    <View        android:layout_marginTop="16dp"        android:background="#EEEEEE"        android:layout_marginLeft="14dp"        android:layout_marginRight="16dp"        android:layout_width="match_parent"        android:layout_height="0.5dp"/></LinearLayout>